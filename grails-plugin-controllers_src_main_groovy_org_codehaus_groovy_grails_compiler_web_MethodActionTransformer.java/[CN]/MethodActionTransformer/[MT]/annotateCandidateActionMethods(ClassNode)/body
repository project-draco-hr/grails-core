{
  for (  MethodNode method : classNode.getMethods()) {
    if (!method.isStatic() && method.isPublic() && method.getAnnotations(ACTION_ANNOTATION_NODE.getClassNode()).isEmpty()&& method.getLineNumber() >= 0) {
      method.setCode(bodyCode(classNode,method.getParameters(),method.getCode()));
      convertToMethodAction(method);
    }
  }
}
