{
  Template t=templateEngine.createTemplate(getUrl());
  Writable w=t.make(model);
  Writer out=null;
  try {
    out=createResponseWriter(response);
    w.writeTo(out);
  }
 catch (  Exception e) {
    out=createResponseWriter(response);
    handleException(e,out,templateEngine,request);
  }
 finally {
    if (out != null)     out.close();
  }
}
