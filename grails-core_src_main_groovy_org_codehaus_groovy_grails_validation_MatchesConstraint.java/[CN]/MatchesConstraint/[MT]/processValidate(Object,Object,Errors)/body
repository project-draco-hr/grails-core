{
  if (propertyValue.toString().matches(regex)) {
    return;
  }
  rejectValue(target,errors,ConstrainedProperty.DEFAULT_DOESNT_MATCH_MESSAGE_CODE,ConstrainedProperty.MATCHES_CONSTRAINT + ConstrainedProperty.INVALID_SUFFIX,new Object[]{constraintPropertyName,constraintOwningClass,propertyValue,regex});
}
