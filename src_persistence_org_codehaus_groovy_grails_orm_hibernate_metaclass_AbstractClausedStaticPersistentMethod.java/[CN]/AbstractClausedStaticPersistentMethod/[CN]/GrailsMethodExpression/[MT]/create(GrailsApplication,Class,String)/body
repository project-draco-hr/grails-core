 else   if (queryParameter.endsWith(NOT_EQUAL)) {
    return new GrailsMethodExpression(application,clazz,calcPropertyName(queryParameter,NOT_EQUAL),NOT_EQUAL,1,isNegation(queryParameter,NOT_EQUAL)){
