{
  if (JdkVersion.getMajorJavaVersion() < JdkVersion.JAVA_15) {
    return false;
  }
  Method m=BeanUtils.findMethod(type.getClass(),"isEnum");
  if (m == null)   return false;
  try {
    Object result=m.invoke(type);
    return result instanceof Boolean && (Boolean)result;
  }
 catch (  Exception e) {
    return false;
  }
}
