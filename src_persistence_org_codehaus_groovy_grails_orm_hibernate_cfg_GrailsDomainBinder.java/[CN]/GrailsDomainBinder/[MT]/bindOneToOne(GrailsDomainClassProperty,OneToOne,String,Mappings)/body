{
  PropertyConfig config=getPropertyConfig(property);
  oneToOne.setConstrained(!property.isOwningSide());
  oneToOne.setForeignKeyType(!property.isOwningSide() ? ForeignKeyDirection.FOREIGN_KEY_FROM_PARENT : ForeignKeyDirection.FOREIGN_KEY_TO_PARENT);
  if (config != null) {
    oneToOne.setLazy(config.getLazy());
  }
 else {
    oneToOne.setLazy(false);
  }
  bindSimpleValue(property,oneToOne,path,mappings);
}
