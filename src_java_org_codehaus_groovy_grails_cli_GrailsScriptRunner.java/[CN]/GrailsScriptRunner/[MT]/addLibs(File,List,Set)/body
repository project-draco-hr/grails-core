{
  if (dir.exists()) {
    File[] files=dir.listFiles();
    for (int i=0; i < files.length; i++) {
      File file=files[i];
      if (file.getName().matches("^.*\\.jar$") && !excludes.contains(file.getName()) && !file.getName().matches("^(standard|jstl)-\\d.*$")) {
        urls.add(file.toURI().toURL());
      }
    }
  }
}
