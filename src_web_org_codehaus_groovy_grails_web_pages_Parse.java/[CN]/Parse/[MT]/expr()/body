{
  if (!finalPass)   return;
  if (LOG.isDebugEnabled())   LOG.debug("parse: expr");
  String text=scan.getToken().trim();
  if (contentType.indexOf("text/html") > -1 && escapeHTML) {
    out.printlnToResponse("HtmlUtils.htmlEscape(" + text + "?.toString())");
  }
 else {
    out.printlnToResponse(text);
  }
}
