{
  if (validator != null) {
    Errors errors=new BindException(domainObject,persistentClass.getName());
    validator.validate(domainObject,errors);
    if (errors.hasErrors()) {
      callback.setErrors(errors);
      callback.setInvoked(false);
      return false;
    }
  }
  template.save(domainObject);
  return true;
}
