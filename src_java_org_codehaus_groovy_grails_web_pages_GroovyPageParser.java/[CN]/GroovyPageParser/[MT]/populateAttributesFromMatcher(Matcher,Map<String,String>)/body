{
  while (m.find()) {
    String name=m.group(2);
    String val=m.group(3);
    name='\"' + name + '\"';
    if (val.startsWith("${") && val.endsWith("}") && val.indexOf("${",2) == -1) {
      val=val.substring(2,val.length() - 1);
    }
 else     if (!(val.startsWith("[") && val.endsWith("]"))) {
      val='\"' + val + '\"';
    }
    attrs.put(name,val);
  }
}
