{
  PrintStream printStream=out;
  if (!(System.out instanceof GrailsConsolePrintStream)) {
    printStream=System.out;
  }
  try {
    if (msg.endsWith(LINE_SEPARATOR)) {
      printStream.print(msg);
    }
 else {
      printStream.println(msg);
    }
    cursorMove=0;
  }
  finally {
    postPrintMessage();
  }
}
