{
  char ch;
switch (c) {
case 'n':
    ch='\n';
  break;
case 'b':
ch='\b';
break;
case 'f':
ch='\f';
break;
case 't':
ch='\t';
break;
case 'r':
ch='\r';
break;
case '\"':
ch='\"';
break;
case '\'':
ch='\'';
break;
case '\\':
ch='\\';
break;
case '/':
ch='/';
break;
default :
throw new IllegalStateException("Unexpected character: " + c + " in "+ str);
}
return ch;
}
