{
  if (msg == null) {
    msg="Done";
  }
  if (lastVerbosePrefix != null) {
    statusBegin(lastVerbosePrefix);
  }
  if (!inStatusRun) {
    statusBegin(msg);
    inStatusRun=false;
  }
 else {
    if (lastWasContinuous) {
      consoleOut.print(' ');
    }
    println(msg);
    inStatusRun=false;
  }
  lastVerbosePrefix=null;
}
