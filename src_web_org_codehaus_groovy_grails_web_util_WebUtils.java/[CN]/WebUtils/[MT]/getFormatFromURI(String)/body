{
  if (uri.endsWith("/")) {
    return null;
  }
  int idx=uri.lastIndexOf('/');
  if (idx > -1) {
    String lastToken=uri.substring(idx + 1,uri.length());
    idx=lastToken.lastIndexOf('.');
    if (idx > -1 && idx != lastToken.length() - 1) {
      return lastToken.substring(idx + 1);
    }
  }
  return null;
}
