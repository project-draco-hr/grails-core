{
  this.dependencyManager=dependencyManager;
  this.pluginName=pluginName;
  this.inherited=inherited;
  if (pluginName != null) {
    DependencyDescriptor pluginDependencyDescriptor=dependencyManager.getPluginDependencyDescriptor(pluginName);
    exported=Metadata.getCurrent().getInstalledPlugins().containsKey(pluginName) || !(pluginDependencyDescriptor instanceof EnhancedDefaultDependencyDescriptor) || ((EnhancedDefaultDependencyDescriptor)pluginDependencyDescriptor).getExported();
  }
 else {
    exported=true;
  }
}
