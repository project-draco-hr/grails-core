{
  if (LOG.isDebugEnabled())   LOG.debug("Tag [" + methodName + "] not found in existing library, found in ["+ tagLibrary.getClass().getName()+ "]. Invoking..");
  Map attrs=Collections.EMPTY_MAP;
  Object body=null;
  if (arguments.length > 0 && arguments[0] instanceof Map)   attrs=(Map)arguments[0];
  if (arguments.length > 1) {
    body=arguments[1];
  }
  Writer originalOut=webRequest.getOut();
  try {
    return GroovyPage.captureTagOutput(tagLibrary,methodName,attrs,body,webRequest,new BeanWrapperImpl(tagLibrary));
  }
  finally {
    webRequest.setOut(originalOut);
  }
}
