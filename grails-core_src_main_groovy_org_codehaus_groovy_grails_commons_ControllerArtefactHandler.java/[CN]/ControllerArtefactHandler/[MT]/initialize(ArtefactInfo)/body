{
  Object cacheSize=grailsApplication.getFlatConfig().get(URL_MAPPING_CACHE_MAX_SIZE);
  if (cacheSize == null) {
    cacheSize=10000;
  }
  uriToControllerClassCache=new ConcurrentLinkedHashMap.Builder<String,GrailsClass>().initialCapacity(500).maximumWeightedCapacity(new Integer(cacheSize.toString())).build();
  artefactInfo=artefacts;
}
