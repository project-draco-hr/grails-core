{
  if (!(constraintParameter instanceof Comparable))   throw new IllegalArgumentException("Parameter for constraint [" + ConstrainedProperty.MIN_CONSTRAINT + "] of property ["+ constraintPropertyName+ "] of class ["+ constraintOwningClass+ "] must implement the interface [java.lang.Comparable]");
  this.minValue=(Comparable)constraintParameter;
  super.setParameter(constraintParameter);
}
