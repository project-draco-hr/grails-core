{
  if (config != null && config.getType() != null) {
    final Object typeObj=config.getType();
    if (typeObj instanceof Class<?>) {
      return ((Class<?>)typeObj).getName();
    }
    return typeObj.toString();
  }
  if (mapping != null) {
    return mapping.getTypeName(property.getType());
  }
  return null;
}
