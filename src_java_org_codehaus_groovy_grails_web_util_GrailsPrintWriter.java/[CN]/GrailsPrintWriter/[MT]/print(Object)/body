{
  if (trouble || obj == null) {
    return;
  }
 else {
    if (obj instanceof CharSequence) {
      try {
        out.append((CharSequence)obj);
      }
 catch (      IOException e) {
        handleIOException(e);
      }
    }
 else     if (obj instanceof StreamCharBuffer) {
      this.write((StreamCharBuffer)obj);
    }
 else {
      write(String.valueOf(obj));
    }
  }
}
