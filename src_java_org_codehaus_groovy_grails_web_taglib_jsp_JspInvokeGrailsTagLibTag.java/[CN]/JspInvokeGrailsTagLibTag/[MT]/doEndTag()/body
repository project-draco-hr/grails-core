{
  if (!bodyInvokation) {
    if (this.tagContent == null)     this.tagContent=sw.toString();
    if (tagContent != null) {
      try {
        jspWriter=pageContext.getOut();
        jspWriter.write(tagContent);
        out.close();
      }
 catch (      IOException e) {
        throw new JspTagException("I/O error writing tag contents [" + tagContent + "] to response out");
      }
    }
  }
  return SKIP_BODY;
}
