{
  final ClassNode returnType=_method.getReturnType();
  MethodNode method=new MethodNode(GrailsControllerClass.METHOD_DISPATCHER_PREFIX + _method.getName(),Modifier.PUBLIC,returnType,ZERO_PARAMETERS,EMPTY_CLASS_ARRAY,addOriginalMethodCall(_method,initializeActionParameters(classNode,_method.getParameters())));
  annotateActionMethod(_method.getParameters(),method);
  return method;
}
