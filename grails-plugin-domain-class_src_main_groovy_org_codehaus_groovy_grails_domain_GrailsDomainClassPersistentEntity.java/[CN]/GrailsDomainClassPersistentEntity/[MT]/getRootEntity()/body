{
  if (isRoot())   return this;
  PersistentEntity parent=getParentEntity();
  if (parent == null) {
    return this;
  }
  while (!parent.isRoot()) {
    PersistentEntity current=parent.getParentEntity();
    if (current != null) {
      parent=current;
    }
 else {
      break;
    }
  }
  return parent;
}
