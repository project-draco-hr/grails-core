{
  if (c instanceof List) {
    try {
      return ((List)c).get(index);
    }
 catch (    IndexOutOfBoundsException ignored) {
    }
  }
 else {
    int j=0;
    for (Iterator i=c.iterator(); i.hasNext(); j++) {
      Object o=i.next();
      if (j == index) {
        return o;
      }
    }
  }
  return null;
}
