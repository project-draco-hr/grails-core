{
  String defaultMessage;
  try {
    if (messageSource != null)     defaultMessage=messageSource.getMessage(code,args,Locale.getDefault());
 else     defaultMessage=(String)DEFAULT_MESSAGES.get(code);
  }
 catch (  NoSuchMessageException nsme) {
    defaultMessage=(String)DEFAULT_MESSAGES.get(code);
  }
  return defaultMessage;
}
