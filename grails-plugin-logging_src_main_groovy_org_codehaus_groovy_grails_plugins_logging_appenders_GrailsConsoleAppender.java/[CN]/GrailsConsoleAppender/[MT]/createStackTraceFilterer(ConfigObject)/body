{
  try {
    stackTraceFilterer=(StackTraceFilterer)GrailsClassUtils.instantiateFromConfig(config,"grails.logging.stackTraceFiltererClass",DefaultStackTraceFilterer.class.getName());
  }
 catch (  Throwable t) {
    LogLog.error("Problem instantiating StackTracePrinter class, using default: " + t.getMessage());
    stackTraceFilterer=new DefaultStackTraceFilterer();
  }
}
