{
  if (s == null) {
    throw new JSONException("Null key.");
  }
  if (this.mode == KEY) {
    try {
      if (this.comma) {
        this.comma();
      }
      this.writer.write(JSONObject.quote(s));
      this.writer.write(':');
      this.comma=false;
      this.mode=OBJECT;
      return this;
    }
 catch (    IOException e) {
      throw new JSONException(e);
    }
  }
  throw new JSONException("Misplaced key.");
}
