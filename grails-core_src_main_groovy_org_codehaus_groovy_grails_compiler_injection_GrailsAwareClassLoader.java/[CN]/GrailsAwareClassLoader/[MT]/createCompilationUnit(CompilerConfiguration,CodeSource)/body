{
  CompilationUnit cu=super.createCompilationUnit(config,source);
  GrailsAwareInjectionOperation operation;
  if (classInjectors == null)   operation=new GrailsAwareInjectionOperation(getResourceLoader());
 else   operation=new GrailsAwareInjectionOperation(getResourceLoader(),classInjectors);
  cu.addPhaseOperation(operation,Phases.CANONICALIZATION);
  return cu;
}
