{
  flush();
  if (CONTENT_LENGTH_COUNTING_ENABLED && bytesCounter != null && response != null && !response.isCommitted()) {
    int size=bytesCounter.size();
    if (size > 0) {
      response.setContentLength(size);
    }
    try {
      response.getWriter().flush();
    }
 catch (    IOException e) {
      handleIOException(e);
    }
  }
}
