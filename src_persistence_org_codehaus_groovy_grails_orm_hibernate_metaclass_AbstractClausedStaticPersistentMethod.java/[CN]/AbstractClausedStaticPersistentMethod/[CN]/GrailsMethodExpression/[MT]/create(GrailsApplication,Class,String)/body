        if (arguments[0] == null)         return Restrictions.isNull(this.propertyName);
        return Restrictions.lt(this.propertyName,arguments[0]);
      }
    }
;
  }
 else   if (queryParameter.endsWith(GREATER_THAN_OR_EQUAL)) {
