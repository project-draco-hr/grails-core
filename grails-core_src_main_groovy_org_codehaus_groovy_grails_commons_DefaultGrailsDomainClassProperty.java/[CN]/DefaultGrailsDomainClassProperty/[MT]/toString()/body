{
  String assType=null;
  if (isManyToMany()) {
    assType="many-to-many";
  }
 else   if (isOneToMany()) {
    assType="one-to-many";
  }
 else   if (isOneToOne()) {
    assType="one-to-one";
  }
 else   if (isManyToOne()) {
    assType="many-to-one";
  }
 else   if (isEmbedded()) {
    assType="embedded";
  }
  return new ToStringBuilder(this).append("name",name).append("type",type).append("persistent",isPersistent()).append("optional",isOptional()).append("association",isAssociation()).append("bidirectional",isBidirectional()).append("association-type",assType).toString();
}
