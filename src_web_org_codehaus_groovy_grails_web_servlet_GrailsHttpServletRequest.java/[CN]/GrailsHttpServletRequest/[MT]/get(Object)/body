{
  if (key != null) {
    String s=key.toString();
    Object result=getRequest().getAttribute(s);
    if (result == null && key.equals(DELEGATE))     return getDelegate();
    if (result == null && requestBean.isReadableProperty(s))     return requestBean.getPropertyValue(s);
 else     return result;
  }
 else   return null;
}
