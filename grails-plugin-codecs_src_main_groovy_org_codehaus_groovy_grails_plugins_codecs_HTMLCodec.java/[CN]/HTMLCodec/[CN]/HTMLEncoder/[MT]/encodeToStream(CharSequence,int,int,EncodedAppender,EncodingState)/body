{
  if (str == null || len <= 0) {
    return;
  }
  CharSequence source;
  if (off == 0 && len == str.length()) {
    source=str;
  }
 else {
    source=str.subSequence(off,off + len);
  }
  String encoded=(String)encode(source);
  if (encoded != null) {
    String encodedStr=String.valueOf(encoded);
    appender.append(this,encodingState,encodedStr,0,encodedStr.length());
  }
}
