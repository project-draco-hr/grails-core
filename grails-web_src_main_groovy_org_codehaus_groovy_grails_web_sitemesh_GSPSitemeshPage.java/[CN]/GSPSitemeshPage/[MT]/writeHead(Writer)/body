{
  if (headBuffer == null) {
    return;
  }
  if (titleCaptured) {
    if (titleBuffer != null) {
      int headlen=headBuffer.length();
      titleBuffer.clear();
      if (headBuffer.length() < headlen) {
        headBuffer.writeTo(out);
        return;
      }
    }
    String headAsString=headBuffer.toString();
    out.write(headAsString.replaceFirst("<title(\\s[^>]*)?>(.*?)</title>",""));
  }
 else {
    headBuffer.writeTo(out);
  }
}
