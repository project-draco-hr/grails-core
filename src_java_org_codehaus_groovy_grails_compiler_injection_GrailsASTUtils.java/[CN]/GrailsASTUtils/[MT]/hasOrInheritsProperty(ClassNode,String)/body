{
  if (hasProperty(classNode,propertyName)) {
    return true;
  }
 else {
    ClassNode parent=classNode.getSuperClass();
    while (parent != null && !getFullName(parent).equals("java.lang.Object")) {
      if (hasProperty(parent,propertyName)) {
        return true;
      }
      parent=parent.getSuperClass();
    }
  }
  return false;
}
