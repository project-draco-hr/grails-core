{
  Integer slashCount=org.springframework.util.StringUtils.countOccurrencesOf(uri,"/");
  for (  Pattern pattern : patterns) {
    Matcher m=pattern.matcher(uri);
    if (m.matches()) {
      UrlMappingInfo urlInfo=createUrlMappingInfo(uri,m,slashCount);
      if (urlInfo != null) {
        return urlInfo;
      }
    }
  }
  return null;
}
