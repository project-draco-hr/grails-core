{
  if (scaffoldViewResolver == null)   throw new IllegalStateException("Property [viewResolver] is required to be set, but is null");
  Object includeUri=request.getAttribute("javax.servlet.include.request_uri");
  String uri;
  if (includeUri != null) {
    uri=(String)includeUri;
  }
 else {
    uri=request.getRequestURI();
  }
  return new ModelAndView(scaffoldViewResolver.resolveViewForUriAndAction(uri,actionName),model);
}
