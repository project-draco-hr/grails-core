{
  final GrailsDomainClassProperty versionProperty=property.getDomainClass().getVersion();
  final boolean isVersion=versionProperty != null && versionProperty.equals(property);
  return !constrainedProperty.hasAppliedConstraint(ConstrainedProperty.NULLABLE_CONSTRAINT) && isConstrainableProperty(property,propertyName) && !property.isIdentity()&& !isVersion;
}
