{
  try {
    List<ClassInjector> injectors=ArtefactTypeAstTransformation.findInjectors(artefactType,GrailsAwareInjectionOperation.getClassInjectors());
    for (    ClassInjector injector : injectors) {
      injector.performInjection(sourceUnit,cNode);
    }
  }
 catch (  RuntimeException e) {
    GrailsConsole.getInstance().error("Error occurred calling AST injector: " + e.getMessage(),e);
    throw e;
  }
}
