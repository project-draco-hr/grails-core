{
  if (newValue instanceof List) {
    deferredProperties.put(currentBeanConfig.getName() + property,new DeferredProperty(currentBeanConfig,property,newValue));
    return true;
  }
 else   if (newValue instanceof Map) {
    deferredProperties.put(currentBeanConfig.getName() + property,new DeferredProperty(currentBeanConfig,property,newValue));
    return true;
  }
  return false;
}
