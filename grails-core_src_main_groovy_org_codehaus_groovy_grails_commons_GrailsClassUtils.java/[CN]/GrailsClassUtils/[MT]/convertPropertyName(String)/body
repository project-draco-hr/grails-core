{
  if (prop.length() == 1) {
    return prop.toLowerCase();
  }
 else   if (Character.isUpperCase(prop.charAt(0)) && Character.isUpperCase(prop.charAt(1))) {
    return prop;
  }
  if (Character.isDigit(prop.charAt(0))) {
    return prop;
  }
  return Character.toLowerCase(prop.charAt(0)) + prop.substring(1);
}
