{
  if (!(constraintParameter instanceof Boolean))   throw new IllegalArgumentException("Parameter for constraint [" + ConstrainedProperty.BLANK_CONSTRAINT + "] of property ["+ constraintPropertyName+ "] of class ["+ constraintOwningClass+ "] must be a boolean value");
  this.blank=((Boolean)constraintParameter).booleanValue();
  super.setParameter(constraintParameter);
}
