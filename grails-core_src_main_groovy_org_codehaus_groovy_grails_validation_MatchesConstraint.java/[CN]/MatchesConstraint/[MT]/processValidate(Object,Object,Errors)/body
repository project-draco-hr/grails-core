{
  if (!propertyValue.toString().matches(regex)) {
    Object[] args=new Object[]{constraintPropertyName,constraintOwningClass,propertyValue,regex};
    rejectValue(target,errors,ConstrainedProperty.DEFAULT_DOESNT_MATCH_MESSAGE_CODE,ConstrainedProperty.MATCHES_CONSTRAINT + ConstrainedProperty.INVALID_SUFFIX,args);
  }
}
