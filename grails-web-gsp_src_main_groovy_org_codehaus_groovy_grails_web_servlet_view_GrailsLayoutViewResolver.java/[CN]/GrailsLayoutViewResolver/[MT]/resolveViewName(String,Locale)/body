{
  View innerView=innerViewResolver.resolveViewName(viewName,locale);
  if (innerView == null) {
    return null;
  }
 else   if (innerView instanceof SmartView && ((SmartView)innerView).isRedirectView()) {
    return innerView;
  }
 else {
    return new GrailsLayoutView(groovyPageLayoutFinder,innerView);
  }
}
