{
  boolean renderView;
  OpenRicoBuilder orb;
  try {
    orb=new OpenRicoBuilder(response);
    renderView=false;
  }
 catch (  IOException e) {
    throw new ControllerExecutionException("I/O error executing render method for arguments [" + callable + "]: "+ e.getMessage(),e);
  }
  orb.invokeMethod("ajax",new Object[]{callable});
  return renderView;
}
