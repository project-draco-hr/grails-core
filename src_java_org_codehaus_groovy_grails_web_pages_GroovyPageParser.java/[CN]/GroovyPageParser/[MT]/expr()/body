{
  if (!finalPass)   return;
  if (LOG.isDebugEnabled())   LOG.debug("parse: expr");
  String text=scan.getToken().trim();
  text=getExpressionText(text);
  if (text.startsWith("(") && text.endsWith(")")) {
    out.printlnToResponse("Codec.encode" + text);
  }
 else {
    out.printlnToResponse("Codec.encode(" + text + ")");
  }
}
