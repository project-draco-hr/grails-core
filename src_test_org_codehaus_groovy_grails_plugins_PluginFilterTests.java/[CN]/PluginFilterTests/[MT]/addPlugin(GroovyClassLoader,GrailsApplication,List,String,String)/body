{
  Class c=gcl.parseClass("class " + className + " {\n"+ "def version = 1.1;\n"+ (dependencies != null ? "def dependsOn = " + dependencies + ";\n" : "")+ "def doWithSpring = {"+ "}\n"+ "def doWithApplicationContext = { ctx ->"+ "}"+ "}");
  DefaultGrailsPlugin p=new DefaultGrailsPlugin(c,app);
  pluginList.add(p);
}
