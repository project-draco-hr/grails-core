{
  Object last=null;
  if (node != null) {
    try {
      int i=((Integer)node);
      while (i-- > 0) {
        last=stack.pop();
        if (BuilderMode.ARRAY == last)         writer.endArray();
        if (BuilderMode.OBJECT == last)         writer.endObject();
      }
    }
 catch (    JSONException e) {
      throw new IllegalArgumentException("invalid element on the stack");
    }
  }
}
