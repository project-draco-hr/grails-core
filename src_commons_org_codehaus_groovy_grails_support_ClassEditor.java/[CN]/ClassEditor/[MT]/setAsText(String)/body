{
  try {
    Class clazz=ClassUtils.resolvePrimitiveClassName(className);
    if (clazz != null) {
      setValue(clazz);
    }
 else {
      setValue(this.classLoader.loadClass(className));
    }
  }
 catch (  ClassNotFoundException e) {
    throw new IllegalArgumentException("Could not load class [" + className + "]!");
  }
}
