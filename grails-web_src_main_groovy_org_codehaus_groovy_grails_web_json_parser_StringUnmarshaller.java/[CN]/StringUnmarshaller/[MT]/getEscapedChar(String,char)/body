{
switch (c) {
case 'n':
    return '\n';
case 'b':
  return '\b';
case 'f':
return '\f';
case 't':
return '\t';
case 'r':
return '\r';
case '\"':
return '\"';
case '\'':
return '\'';
case '\\':
return '\\';
case '/':
return '/';
default :
throw new IllegalStateException("Unexpected character: " + c + " in "+ str);
}
}
