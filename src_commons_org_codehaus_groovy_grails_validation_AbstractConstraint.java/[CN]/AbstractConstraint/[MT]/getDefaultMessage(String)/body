{
  String defaultMessage;
  try {
    if (messageSource != null)     defaultMessage=messageSource.getMessage(code,null,LocaleContextHolder.getLocale());
 else     defaultMessage=(String)ConstrainedProperty.DEFAULT_MESSAGES.get(code);
  }
 catch (  Exception e) {
    defaultMessage=(String)ConstrainedProperty.DEFAULT_MESSAGES.get(code);
  }
  return defaultMessage;
}
