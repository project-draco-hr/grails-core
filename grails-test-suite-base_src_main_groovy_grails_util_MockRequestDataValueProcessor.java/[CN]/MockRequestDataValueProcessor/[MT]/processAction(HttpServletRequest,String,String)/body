{
  if (action.indexOf("requestDataValueProcessorParamName=paramValue") > -1) {
    action=action.replace("?requestDataValueProcessorParamName=paramValue&","?");
    action=action.replace("?requestDataValueProcessorParamName=paramValue","");
    action=action.replace("&requestDataValueProcessorParamName=paramValue","");
  }
  return action;
}
