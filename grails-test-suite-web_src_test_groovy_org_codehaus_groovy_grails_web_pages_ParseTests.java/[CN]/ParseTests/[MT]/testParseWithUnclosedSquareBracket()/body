{
  String output=parseCode("myTest2","<g:message code=\"[\"/>").generatedGsp;
  String expected=makeImports() + "\n" + "class myTest2 extends GroovyPage {\n"+ "public String getGroovyPageFileName() { \"myTest2\" }\n"+ "public Object run() {\n"+ "def out = getOut()\n"+ "def codecOut = getCodecOut()\n"+ "registerSitemeshPreprocessMode()\n"+ "invokeTag('message','g',1,['code':evaluate('\"[\"', 1, it) { return \"[\" }] as GroovyPageAttributes,-1)\n"+ "}\n"+ GSP_FOOTER;
  assertEquals(trimAndRemoveCR(expected),trimAndRemoveCR(output));
}
