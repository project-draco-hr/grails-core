{
  modifiedTimes=new long[watchedResources.length];
  for (int i=0; i < watchedResources.length; i++) {
    Resource r=watchedResources[i];
    URLConnection c=r.getURL().openConnection();
    c.setDoInput(false);
    c.setDoOutput(false);
    c.connect();
    modifiedTimes[i]=c.getLastModified();
  }
}
