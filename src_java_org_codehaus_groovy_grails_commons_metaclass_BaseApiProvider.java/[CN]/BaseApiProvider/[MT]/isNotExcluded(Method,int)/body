{
  final String name=method.getName();
  if (EXCLUDED_METHODS.contains(name))   return false;
  Integer parameterCount=METHOD_CLOSURES.get(name);
  return Modifier.isPublic(modifiers) && !Modifier.isAbstract(modifiers) && !name.contains("$")&& !(parameterCount != null && (parameterCount == method.getParameterTypes().length));
}
