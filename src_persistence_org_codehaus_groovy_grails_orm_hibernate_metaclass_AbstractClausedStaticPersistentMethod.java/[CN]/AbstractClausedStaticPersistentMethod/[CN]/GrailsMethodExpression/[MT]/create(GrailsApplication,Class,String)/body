{
  if (queryParameter.endsWith(LESS_THAN_OR_EQUAL)) {
    return new GrailsMethodExpression(application,clazz,calcPropertyName(queryParameter,LESS_THAN_OR_EQUAL),LESS_THAN_OR_EQUAL,1,isNegation(queryParameter,LESS_THAN_OR_EQUAL)){
      Criterion createCriterion(){
        return Restrictions.le(this.propertyName,arguments[0]);
      }
    }
;
  }
 else   if (queryParameter.endsWith(LESS_THAN)) {
    return new GrailsMethodExpression(application,clazz,calcPropertyName(queryParameter,LESS_THAN),LESS_THAN,1,isNegation(queryParameter,LESS_THAN)){
