{
  if (pluginManager == null) {
    return null;
  }
  if (!uri.startsWith(PATH_TO_WEB_INF_VIEWS))   uri=GrailsResourceUtils.appendPiecesForUri(PATH_TO_WEB_INF_VIEWS,uri);
  for (  GrailsPlugin plugin : pluginManager.getAllPlugins()) {
    if (!(plugin instanceof BinaryGrailsPlugin)) {
      continue;
    }
    Class<?> viewClass=((BinaryGrailsPlugin)plugin).resolveView(uri);
    if (viewClass != null) {
      return new GroovyPageCompiledScriptSource(uri,viewClass);
    }
  }
  return null;
}
