{
  if (isArtefactClass(aClass)) {
    if (log.isDebugEnabled()) {
      log.debug("[" + aClass.getName() + "] is a "+ type+ " class.");
    }
    return true;
  }
 else {
    if (log.isDebugEnabled()) {
      log.debug("[" + aClass.getName() + "] is not a "+ type+ " class.");
    }
    return false;
  }
}
